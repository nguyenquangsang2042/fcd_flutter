                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayoutReport"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayoutIconReport"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_report"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Report"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <TextView
                                android:textStyle="bold"
                                android:text="Report"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/tv_PilotMain_Report"
                                android:textColor="@color/clVNAText2"
                                android:fontFamily="@string/light_font"
                                android:gravity="center_horizontal"
                                android:tag="MAIN_MENU_REPORT" />

                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
             <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:background="@color/clLineMain" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:id="@+id/linearLayoutFAQs"
                android:layout_weight="1"
                android:weightSum="2">
               
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayoutFAQs"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayoutIconFAQs"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_faqs"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_FAQs"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <TextView
                                android:textStyle="bold"
                                android:text="FAQs"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/tv_PilotMain_Help"
                                android:textColor="@color/clVNAText2"
                                android:fontFamily="@string/light_font"
                                android:gravity="center_horizontal"
                                android:tag="MAIN_MENU_FAQS" />

                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="@dimen/line_height"
                    android:layout_height="match_parent"
                    android:id="@+id/view1"
                    android:background="@color/clLineMain" />
                <LinearLayout
                   
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayoutDontRelate"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayoutIconReport"
                        android:layout_weight="2">
                        <ImageView
                            android:visibility="invisible"
                            android:src="@drawable/icon_reply"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_DontRelate"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:visibility="invisible"
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <TextView
                                android:textStyle="bold"
                                android:text="Report"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/tv_PilotMain_DontRelate"
                                android:textColor="@color/clVNAText2"
                                android:fontFamily="@string/light_font"
                                android:gravity="center_horizontal"
                                android:tag="MAIN_MENU_REPORT" />

                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </android.support.v4.widget.SwipeRefreshLayout>
