
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/lnPilotMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#006885"
    android:weightSum="9">

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/linearLayout1"
        android:layout_weight="3">
        <android.support.v4.view.ViewPager
            android:id="@+id/ViewPagerBanner"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <ImageView
            android:scaleType="fitXY"
            android:visibility="gone"
            android:id="@+id/img_back_drop"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/main_background">
        </ImageView>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/gradient_shape">
        </ImageView>
        <LinearLayout
            android:id="@+id/linear_flight_info"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginTop="0.0dp">
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <refractored.controls.CircleImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:id="@+id/img_PilotMain_Avata"
                    android:backgroundTint="#00000000"
                    android:tint="#00000000"
                    android:padding="3dp"
                    app:civ_border_width="1dp"
                    app:civ_border_color="#ffffffff"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_avatar64"/>
                <TextView
                    android:fontFamily="@string/normal_font"
                    android:textSize="@dimen/content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:text=""
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tv_PilotMain_Name"
                    android:layout_toRightOf="@id/img_PilotMain_Avata"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/white" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <refractored.controls.CircleImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:id="@+id/img_QRCode_GetInfo"
                    android:backgroundTint="#00000000"
                    android:tint="#00000000"
                    android:padding="3dp"
                    app:civ_border_width="1dp"
                    app:civ_border_color="#ffffffff"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_qrcode"/>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/dotsBannerContainer"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="5dp">

            <android.support.design.widget.TabLayout
                android:id="@+id/tabLayoutBanner"
                android:layout_alignParentBottom="true"
                android:layout_width="match_parent"
                android:layout_height="15dp"
                app:tabBackground="@drawable/tab_selector_banner"
                app:tabGravity="center"
                app:tabIndicatorHeight="0dp" />

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linear_PilotMain_Go"
            android:layout_alignParentBottom="true"
            android:visibility="visible">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="25dp">
                <ImageView
                    android:visibility="invisible"
                    android:src="@drawable/icon_plane30gray"
                    android:layout_width="30dp"
                    android:layout_height="match_parent"
                    android:id="@+id/img_PilotMain_plane"
                    android:tint="#ffffffff"
                    android:layout_marginLeft="5dp" />
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout9"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="20dp">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_weight="1"
                            android:text=""
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:id="@+id/tv_PilotMain_Address"
                            android:textColor="@color/white"
                            android:maxLines="1"
                            android:ellipsize="end" />
                        <ImageView
                            android:visibility="gone"
                            android:layout_marginRight="10dp"
                            android:layout_gravity="center_vertical"
                            android:clickable="false"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/icon_signout"
                            android:layout_width="wrap_content"
                            android:id="@+id/img_PilotMain_Close"
                            android:tint="@color/white"
                            android:layout_height="match_parent"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_weight="1"
                            android:text=""
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:id="@+id/tv_PilotMain_Date"
                            android:layout_marginTop="5dp"
                            android:textColor="@color/white"
                            android:ellipsize="end"
                            android:maxLines="1" />
                        <TextView
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="5dp"
                            android:textSize="@dimen/content"
                            android:id="@+id/tv_app_version"
                            android:textColor="@color/white"
                            android:text=""
                            android:fontFamily="@string/normal_font"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <android.support.v4.widget.SwipeRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="6"
        android:id="@+id/swipe_PilotMain">
        <GridView
              android:verticalSpacing="0dp"
    android:horizontalSpacing="0dp"
    android:stretchMode="columnWidth"
            android:numColumns="2"
            android:id="@+id/gridViewMenu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <LinearLayout
            android:visibility="gone"
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout2"
            android:weightSum="6">
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:id="@+id/linearLayoutSafetyAndNews"
                android:layout_weight="1">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearSafety"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout7"
                        android:layout_weight="2">
                        <ImageView
                            android:tint="@color/clVNAText"
                            android:src="@drawable/icon_shield2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Notification"
                            android:padding="12dp"
							 />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:tag="MAIN_MENU_NOTIFICATIONS"
                            android:textStyle="bold"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:text="Safety"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_Notification"
                            android:gravity="center_horizontal"
						/>
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="@dimen/line_height"
                    android:layout_height="match_parent"
                    android:id="@+id/view1"
                    android:background="@color/clLineMain" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearNews"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout7"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/ic_news"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_News"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:tag="MAIN_MENU_NEWS"
                            android:textStyle="bold"
                            android:text="News"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_News"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:gravity="center_horizontal"						
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:id="@+id/view2"
                android:background="@color/clLineMain" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:id="@+id/linearLayoutLicenceAndSchedule"
                android:layout_weight="1"
                android:weightSum="2">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout6"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout7"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_lisence30"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Licence"
                            android:padding="12dp"
                            android:layout_gravity="center"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:textStyle="bold"
                            android:text="Licence"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_Licence"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:gravity="center_horizontal"
                            android:tag="MAIN_MENU_LICENSE" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="@dimen/line_height"
                    android:layout_height="match_parent"
                    android:id="@+id/view1"
                    android:background="@color/clLineMain" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayoutSchedule"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayoutIconPlane"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_plane30gray"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Schedule"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:textStyle="bold"
                            android:text="Schedule"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_Schedule"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:gravity="center_horizontal"
                            android:tag="MAIN_MENU_SCHEDULE" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:id="@+id/viewline_3"
                android:background="@color/clLineMain" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:id="@+id/linearLayoutTicketAndTraining"
                android:layout_weight="1"
                android:weightSum="2">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout6"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout7"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_ticket_booking30"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Ticket"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:textStyle="bold"
                            android:text="Ticket request"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_Ticket"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:gravity="center_horizontal"
                            android:tag="MAIN_MENU_REQUEST_TICKET" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="@dimen/line_height"
                    android:layout_height="match_parent"
                    android:id="@+id/view1"
                    android:background="@color/clLineMain" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayoutTraining"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayoutIconTraining"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_training"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Training"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:textStyle="bold"
                            android:text="Training"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_Training"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:gravity="center_horizontal"
                            android:tag="MAIN_MENU_TRAINING" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:id="@+id/viewline_4"
                android:background="@color/clLineMain" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:id="@+id/linearLayoutPayrolAndLibrary"
                android:layout_weight="1"
                android:weightSum="2">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout6"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout7"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_payroll"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Payroll"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:textStyle="bold"
                            android:text="Payroll"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_Payroll"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:gravity="center_horizontal"
                            android:tag="MAIN_MENU_REQUEST_PAYROLL" />
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="@dimen/line_height"
                    android:layout_height="match_parent"
                    android:id="@+id/view1"
                    android:background="@color/clLineMain" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayoutLibrary"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout7"
                        android:layout_weight="2">
                        <ImageView
                            android:src="@drawable/icon_library30"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/img_PilotMain_Library"
                            android:padding="12dp"
                            android:tint="@color/clVNAText" />
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:minWidth="25px"
                        android:minHeight="25px"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:id="@+id/linearLayout8"
                        android:layout_weight="1">
                        <TextView
                            android:textStyle="bold"
                            android:text="Library"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/tv_PilotMain_Library"
                            android:textColor="@color/clVNAText2"
                            android:fontFamily="@string/light_font"
                            android:gravity="center_horizontal"
                            android:tag="MAIN_MENU_LIBRARY" />
                    </LinearLayout>
                </LinearLayout>



            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_height"
                android:id="@+id/viewline_5"
                android:background="@color/clLineMain" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"
                android:id="@+id/linearLayoutContactAndFAQs"
                android:layout_weight="1"
                android:weightSum="2">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0.1dp"
                    android:layout_height="match_parent"
                    android:id="@+id/linearLayout6"
                    android:layout_weight="1"
                    android:weightSum="3"
                    android:background="@color/clBlueMain">
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/linearLayout6"
                            android:weightSum="3">
                            <LinearLayout
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:id="@+id/linearLayout7"
                                android:layout_weight="2">
                                <ImageView
                                    android:src="@drawable/icon_user2"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/img_PilotMain_Contacts"
                                    android:padding="12dp"
                                    android:tint="@color/clVNAText" />
                            </LinearLayout>
                            <LinearLayout
                                android:orientation="vertical"
                                android:minWidth="25px"
                                android:minHeight="25px"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:id="@+id/linearLayout8"
                                android:layout_weight="1">
                                <TextView
                                    android:text="Contacts"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:id="@+id/tv_PilotMain_Contacts"
                                    android:textColor="@color/clVNAText2"
                                    android:fontFamily="@string/light_font"
                                    android:gravity="center_horizontal"
                                    android:tag="MAIN_MENU_CONTACT"
                                    android:textStyle="bold"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:visibility="visible"
                            android:id="@+id/linear_PilotMain_Pro"
                            android:gravity="center"
                            android:minWidth="25px"
                            android:minHeight="25px">
                            <ProgressBar
                                style="?android:attr/progressBarStyleSmall"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:max="100"
                                android:progress="0"
                                android:indeterminate="true"
                                android:id="@+id/progressBar1" />
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
                <View
                    android:layout_width="@dimen/line_height"
                    android:layout_height="match_parent"
                    android:id="@+id/view1"
                    android:background="@color/clLineMain" />
