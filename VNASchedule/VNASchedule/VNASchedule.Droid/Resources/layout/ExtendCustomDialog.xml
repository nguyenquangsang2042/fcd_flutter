<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_marginBottom="10dp"
    android:padding="5dp"
    android:background="@color/transparent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity ="center">
    <LinearLayout
        android:id="@+id/ln_mainPop"
        android:padding="5dp"
        android:gravity ="center"
        android:background="@drawable/linear_border"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/ln_training"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_training"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:padding="10dp"
                    android:clickable="true"
                    android:textSize="15sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Training"
                    android:id="@+id/txt_training"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ln_license"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_lisence30"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:padding="10dp"
                    android:clickable="true"
                    android:textSize="15sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="License"
                    android:id="@+id/txt_license"
                    android:textColor="@color/clBlueMain"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ln_library"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_library30"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:padding="10dp"
                    android:clickable="true"
                    android:textSize="15sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Library"
                    android:id="@+id/txt_library"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ln_request"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_ticket_booking30"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:padding="10dp"
                    android:clickable="true"
                    android:textSize="15sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Request Ticket"
                    android:id="@+id/txt_request_ticket"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ln_payroll"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_payroll"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:padding="10dp"
                    android:clickable="true"
                    android:textSize="15sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Payroll"
                    android:id="@+id/txt_payroll"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ln_contacts"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/img_contact"
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_user2"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:padding="10dp"
                    android:clickable="true"
                    android:textSize="15sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Contacts"
                    android:id="@+id/txt_contact"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>


        </LinearLayout>
         <LinearLayout
            android:id="@+id/ln_report"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
             <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_report"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:padding="10dp"
                    android:clickable="true"
                    android:textSize="15sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Report"
                    android:id="@+id/txt_report"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/ln_faqs"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/icon_faqs"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:clickable="true"
                    android:textSize="15sp"
                    android:padding="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="FAQs"
                    android:id="@+id/txt_faqs"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>

        </LinearLayout>
       
        <LinearLayout
            android:id="@+id/ln_application"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:tint="@color/clVNAText"
                    android:layout_marginLeft="5dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/ic_menu_application"
                    android:layout_width="25dp"
                    android:layout_height="25dp">
                </ImageView>
                <TextView
                    android:clickable="true"
                    android:textSize="15sp"
                    android:padding="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Application"
                    android:id="@+id/txt_application"
                    android:textColor="@color/clBlueMain"/>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
    <TextView
        android:id="@+id/txt_cancel"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        android:background="@drawable/linear_border"
        android:clickable="true"
        android:textSize="15sp"
        android:textStyle = "bold"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Cancel"
        android:textColor="@color/clBlueMain"/>
</LinearLayout>
