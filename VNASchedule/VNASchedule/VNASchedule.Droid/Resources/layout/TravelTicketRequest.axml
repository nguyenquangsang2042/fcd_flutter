                    </LinearLayout>
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/linearLayout4"
                                android:padding="5dp">
                                <TextView
                                    android:text="Working pattern: "
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView2"
                                    android:textColor="@android:color/background_dark" />
                                <TextView
                                    android:text="Text"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:id="@+id/tv_TravelTicketRequest_WorkingPattern"
                                    android:textColor="@android:color/background_dark"
                                    android:maxLines="1"
                                    android:ellipsize="end" />
                            </LinearLayout>
                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/linearLayout4"
                                android:padding="5dp">
                                <TextView
                                    android:text="Ticket expected date: "
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/textView2"
                                    android:textColor="@android:color/background_dark" />
                                <TextView
                                    android:text="Text"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:id="@+id/tv_TravelTicketRequest_ExpectedDate"
                                    android:textColor="@android:color/background_dark"
                                    android:ellipsize="end"
                                    android:maxLines="1" />
                            </LinearLayout>
                        </LinearLayout>
                        <RelativeLayout
                            android:id="@+id/relative_TravelTicketRequest_Status"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:visibility="visible">
                            <TextView
                                android:text="Rejected"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/tv_TravelTicketRequest_Status"
                                android:textColor="@color/clRedMain"
                                android:rotation="-20"
                                android:gravity="center"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="50dp"
                                android:textSize="16sp"
                                android:padding="5dp" />
                        </RelativeLayout>
                    </FrameLayout>
