					android:tint="@color/white"
                    android:src="@drawable/icon_back30"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:id="@+id/img_TravelTicketRequest_Back"
                    android:padding="10dp"
                    android:layout_centerVertical="true" />
                <TextView
                    android:text="Request detail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tv_TravelTicketRequest_Title"
                    android:textColor="@color/white"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:textSize="18sp"
                    android:tag="TITLE_REQUEST_DETAIL" />
                <TextView
                    android:text="Save"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tv_TravelTicketRequest_Save"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:padding="10dp"
                    android:textColor="@color/white" />
            </RelativeLayout>
